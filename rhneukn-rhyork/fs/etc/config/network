
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '104.206.0.17'
	option ip6addr 'FDCA:FFEE:BABE:0:29F:DBFF:FEE5:F578/64'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option netmask '255.255.255.240'

config interface 'wireless0'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option ip6addr 'FDCA:FFEE:BABE:0:29F:DBFF:FEE4:F578/64'
	option netmask '255.0.0.0'
	option ipaddr '104.206.0.8'
	option proto 'static'

