	option proto 'dhcpv6'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0t 2 3 4 5'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option ports '0t 1'

config switch_vlan
	option device 'switch0'
	option vlan '10'
	option ports '0t 3t'
	option vid '10'
				
config switch_vlan
	option device 'switch0'
	option vlan '20'
	option ports '0t 4t'
	option vid '20'
				
