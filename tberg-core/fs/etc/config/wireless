config wifi-device  radio0
	option channel  13
	option country  DE
	option diversity 1
	option htmode	HT20
	option hwmode	11ng
	option path	'pci0000:00/0000:00:12.0'
	option txpower 8
	option type     mac80211

config wifi-iface
	option device   radio0
	option encryption none
	option mode     ap
	option network  dhcp
	option ssid     'berlin.freifunk.net'

config wifi-iface
	option bssid 'D2:CA:FF:EE:BA:BE'
	option device 'radio0'
	option ifname 'wlan0-adhoc-2'
	option mcast_rate '6000'
	option mode 'adhoc'
	option network 'ffwifi0'
	option ssid 'intern-ch13.freifunk.net'

config wifi-device  radio1
	option type     mac80211
	#option channel  36
	option hwmode	11na
	option diversity 1
	#option doth	1
	option path	'pci0000:00/0000:00:13.0'
	option htmode	HT20
	option country  DE

config wifi-iface
	option device   radio1
	option encryption none
	option mode     sta
	option network  'dk0tu'
	option ssid     'dk0tu-freifunk-w'

config wifi-device  radio2
	option type     mac80211
	option channel  130
	option hwmode	11na
	option diversity 1
	#option doth	1
	option path	'pci0000:00/0000:00:14.0'
	option htmode	HT20
	option country  DE
	option disabled 1

config wifi-iface
	option device   radio2
	option encryption none
	option mode     sta
	option network  potsdam
	option ssid     'intern-potsdam.freifunk.net'

config wifi-device  radio3
	option type     mac80211
	option channel  100
	option hwmode	11na
	option diversity 1
	#option doth	1
	option path	'pci0000:00/0000:00:16.0'
	option htmode	HT20
	option country  DE
	option disabled 1

config wifi-iface
	option device   radio3
	option encryption none
	option mode     sta
	option network  spandau
	option ssid     'intern-spandau.freifunk.net'

config wifi-device  radio4
	option channel  36
	option country  DE
	option disabled 1
	option diversity 1
	option htmode	HT20
	option hwmode	11na
	option path	'pci0000:00/0000:00:17.0'
	option type     mac80211

config wifi-iface
	option device   radio4
	option encryption none
	option mode     ap
	option network  dhcp
	option ssid     'berlin.freifunk.net'

config wifi-iface                                    
        option bssid '02:36:CA:FF:EE:EE'             
        option device 'radio4'                       
        option ifname 'wlan4-adhoc-5'
        option mcast_rate '12000'                     
        option mode 'adhoc'                          
        option network 'ffwifi1'                   
        option ssid 'intern-ch36.freifunk.net'       

