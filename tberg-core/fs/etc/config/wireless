
config wifi-device 'radio0'
	option channel '13'
	option country 'DE'
	option distance '500'
	option diversity '1'
	option htmode 'HT20'
	option hwmode '11ng'
	option path 'pci0000:00/0000:00:12.0'
	option txpower '8'
	option type 'mac80211'

config wifi-iface
	option device 'radio0'
	option encryption 'none'
	option mode 'ap'
	option network 'dhcp'
	option ssid 'berlin.freifunk.net'

config wifi-iface
	option bssid 'D2:CA:FF:EE:BA:BE'
	option device 'radio0'
	option ifname 'wlan0-adhoc-2'
	option mcast_rate '6000'
	option mode 'adhoc'
	option network 'ffwifi0'
	option ssid 'intern-ch13.freifunk.net'

config wifi-device 'radio1'
	option country 'DE'
	option distance '6000'
	option diversity '1'
	option htmode 'HT20'
	option hwmode '11na'
	option path 'pci0000:00/0000:00:13.0'
	option type 'mac80211'

config wifi-iface
	option device 'radio1'
	option encryption 'none'
	option mode 'sta'
	option network 'dk0tu'
	option ssid 'dk0tu-freifunk-w'

config wifi-device 'radio2'
	option channel '116'
	option country 'DE'
	option disabled '1'
	option distance '9000'
	option diversity '1'
	option htmode 'HT20'
	option hwmode '11na'
	option path 'pci0000:00/0000:00:14.0'
	option type 'mac80211'

config wifi-iface
	option device 'radio2'
	option encryption 'none'
	option mode 'sta'
	option network 'beuth'
	option ssid 'BHT-TBERG'

config wifi-device 'radio3'
	option type 'mac80211'
	#option channel '100'
	option hwmode '11na'
	option diversity '1'
	option path 'pci0000:00/0000:00:16.0'
	option htmode 'HT20'
	option country 'DE'
	option disabled '0'

config wifi-iface
	option device 'radio3'
	option encryption 'psk2'
	option mode 'sta'
	option network 'beuth'
	option ssid 'BHT-TBERG'

config wifi-device 'radio4'
	option channel '116'
	option country 'DE'
	option disabled '1'
	option distance '9000'
	option diversity '1'
	option htmode 'HT20'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:17.0'
	option txpower '20'
	option type 'mac80211'

config wifi-iface
	option device 'radio4'
	option encryption 'none'
	option mode 'sta'
	option network 'beuth'
	option ssid 'BHT-TBERG'

