
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fda3:1908:f150::/48'

config interface 'lan'
	option type 'bridge'
	option proto 'static'
	option macaddr 'f8:1a:67:a6:0e:55'
	option ip6assign '64'
	option ipaddr '104.0.2.9'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option netmask '255.255.255.240'
	option ifname 'eth0.100'
	list ip6prefix '2001:470:5038:8C48:0:0:0:0/61'

config interface 'wan'
	option ifname 'eth0.2'
	option macaddr 'f8:1a:67:a6:0e:58'
	option proto 'static'
	option peerdns '0'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option dns '8.8.8.8'

config interface 'wan6'
	option ifname '@wan'
	option proto 'dhcpv6'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
