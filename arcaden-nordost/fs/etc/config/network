
config interface 'loopback'
	option ifname 'lo'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option proto 'static'

config interface 'lan'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '104.204.0.34'
	option netmask '255.255.255.240'
	option ip6assign '64'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	list ip6prefix '2001:470:5038:86E8:0:0:0:0/61'

config interface 'wan'
	option ifname 'eth1'
	option ip6assign '64'
	option ipaddr '192.168.42.1'
	option netmask '255.255.255.0'
	option proto 'static'

config globals 'globals'
	option ula_prefix 'fde1:2203:1087::/48'

