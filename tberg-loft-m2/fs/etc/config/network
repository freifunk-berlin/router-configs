
config interface 'loopback'
	option ifname 'lo'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option proto 'static'

config interface 'batmesh'
	option ifname 'bat0'
	option ip6assign '64'
	option proto 'static'
	option type 'bridge'

config interface 'fflan'
	option ifname 'eth1'
	option ip6assign '64'
	option ipaddr '10.31.3.14'
	option mtu '1532'
	option netmask '255.255.255.255'
	option proto 'static'

config interface 'fflanbat'
	option ifname '@fflan'
	option mesh 'bat0'
	option mtu '1532'
	option proto 'batadv'

config interface 'tunl0'
	option ifname 'tunl0'
	option proto 'none'

config rule 'olsr_allif_ipv4'
	option lookup 'olsr'
	option priority '1000'

config rule 'localnets_allif_ipv4'
	option lookup 'localnets'
	option priority '2000'

config rule 'olsr_tunnel_tunl0_ipv4'
	option in 'tunl0'
	option lookup 'olsr-tunnel'
	option priority '19999'

config rule 'olsr_default_tunl0_ipv4'
	option in 'tunl0'
	option lookup 'olsr-default'
	option priority '20000'

config rule 'olsr_default_unreachable_tunl0_ipv4'
	option action 'unreachable'
	option in 'tunl0'
	option priority '20001'

config interface 'ffvpn'
	option ifname 'ffvpn'
	option proto 'none'

config interface 'wireless0'
	option ip6assign '64'
	option ipaddr '10.31.3.13'
	option mtu '1532'
	option netmask '255.255.255.255'
	option proto 'static'

config interface 'wireless0bat'
	option ifname '@wireless0'
	option mesh 'bat0'
	option mtu '1532'
	option proto 'batadv'

config interface 'lan'
	option dns '85.214.20.141 213.73.91.35 194.150.168.168'
	option ifname 'eth0'
	option ip6assign '64'
	option ip6prefix '2001:bf7:800:20::/60'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option proto 'static'
	option type 'bridge'

