
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '104.88.0.26'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option netmask '255.255.255.252'
	option ip6addr_fdca 'fdca:ffee:babe:f371::1/64'
	option ip6addr '2002:4e35:28e7:9920::1/64'

config interface 'wireless0'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option netmask '255.0.0.0'
	option ipaddr '104.88.0.22'
	option proto 'static'
	option ip6addr_fdca 'fdca:ffee:babe:867::1/64'
	option ip6addr '2002:4e35:28e7:1a6d::1/64'

