
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd69:d842:ffu0::/48'

config interface 'lan'
	option ifname 'eth0.1'
	option proto 'static'
	option ipaddr '192.168.42.1'
	option netmask '255.255.255.0'
	option macaddr 'f8:1a:67:a6:0b:df'
	option ip6assign '64'
	list ip6prefix '2001:470:5038:4C80:0:0:0:0/61'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0t 2 3 4 5'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option ports '0t 1'

config interface 'olsr'
	option proto 'static'
	option ifname 'eth0.2'
	option ipaddr '104.193.0.110'
	option netmask '255.255.255.252'
	option ip6assign '64'
	option macaddr 'f8:1a:67:a6:0b:e2'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'

config interface 'adhoc'
	option mtu '1528'
	option proto 'batadv'
	option mesh 'bat0'
	option ip6assign '64'

config interface 'mesh'
	option ifname 'bat0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '104.193.23.1'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option macaddr 'f8:1a:67:a6:0b:45'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'

