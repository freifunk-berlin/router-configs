
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0'
	option type 'bridge'
	option proto 'static'
	option ip6assign '64'
	option ipaddr '104.206.1.2'
	option netmask '255.0.0.0'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'

config globals 'globals'
	option ula_prefix 'fda2:f19d:9102::/48'

config interface 'wireless0'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option ip6assign '64'
	option netmask '255.0.0.0'
	option ipaddr '104.206.1.18'
	option proto 'static'

config interface 'wireless0dhcp'
	option ipaddr '104.206.1.129'
	option netmask '255.255.255.192'
	option ip6assign '64'
	option proto 'static'

