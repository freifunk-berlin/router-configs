
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11na'
	option path 'pci0000:00/0000:00:00.0'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option disabled '0'
	option noscan '1'
	option country '00'
	option doth '0'
	option distance '2000'
	option diversity '1'
	option channel '136'
	option htmode 'HT20'

config wifi-iface
	option bgscan '0'
	option bssid '12:36:CA:FF:EE:EE'
	option device 'radio0'
	option encryption 'none'
	option ifname 'wlan0-adhoc-5'
	option mode 'adhoc'
	option network 'wireless0'
	option probereq '1'
	option ssid 'ch136.freifunk.net'
	option sw_merge '1'

config wifi-iface
	option device 'radio0'
	option encryption 'none'
	option mode 'ap'
	option network 'batmesh'
	option ssid 'berlin-5ghz.freifunk.net'

