
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '116'
	option macaddr 'dc:9f:db:94:0e:49'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option disabled '0'
	option country 'DE'
	option noscan '1'
	option distance '8000'
	option outdoor '1'
	option htmode 'HT40+'
	option diversity '1'
	option hwmode '11na'
	option txpower '16'

config wifi-iface
	option bgscan '0'
	option bssid '12:16:CA:FF:EE:EE'
	option device 'radio0'
	option encryption 'none'
	option mode 'adhoc'
	option network 'wireless0'
	option probereq '1'
	option ssid 'ch116.freifunk.net'
	option sw_merge '1'

