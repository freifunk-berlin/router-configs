config interface 'fflanbat'
	option ifname '@fflan'
	option mesh 'bat0'
	option mtu '1532'
	option proto 'batadv'

config interface 'fluxfm'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
