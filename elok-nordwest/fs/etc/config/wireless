
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11ng'
	option macaddr 'a0:f3:c1:75:8f:aa'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option disabled '0'
	option country 'DE'
	option noscan '1'
	option diversity '1'
	option htmode 'HT20'
	option channel '13'

