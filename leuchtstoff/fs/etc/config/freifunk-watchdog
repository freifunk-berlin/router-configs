
config process
	option process 'dropbear'
	option initscript '/etc/init.d/dropbear'

config process
	option process 'crond'
	option initscript '/etc/init.d/cron'

config process
	option initscript '/etc/init.d/openvpn'
	option process 'openvpn'

config process
	option initscript '/etc/init.d/openvpn'
	option process 'openvpn'

