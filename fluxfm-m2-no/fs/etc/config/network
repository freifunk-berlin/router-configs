
config interface 'loopback'
	option ifname 'lo'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option proto 'static'

config interface 'batmesh'
        option ifname 'bat0'
        option proto 'none'
        option type 'bridge'

config interface 'lan'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option ifname 'eth0'
	option ip6assign '64'
	option ip6prefix '2001:bf7:830:1320::/60'
	option ipaddr '104.192.7.3'
	option mtu '1532'
	option netmask '255.255.255.240'
	option proto 'static'
	option type 'bridge'

config interface 'lanbat'
	option ifname '@lan'
	option mesh 'bat0'
	option proto 'batadv'

config interface 'wan'
	option ifname 'eth1'
	option proto 'dhcp'

