
config interface 'wan'
	option classgroup 'Default'
	option enabled '0'

config classify
	option ports '22,53'
	option target 'Priority'

config classify
	option ports '20,21,25,80,110,443,993,995'
	option proto 'tcp'
	option target 'Normal'

config classify
	option ports '5190'
	option target 'Express'

config default
	option pktsize '-500'
	option proto 'udp'
	option target 'Express'

config reclassify
	option proto 'icmp'
	option target 'Priority'

config default
	option portrange '1024-65535'
	option target 'Bulk'

config reclassify
	option mark '!Bulk'
	option pktsize '-128'
	option proto 'tcp'
	option target 'Priority'
	option tcpflags 'SYN'

config reclassify
	option mark '!Bulk'
	option pktsize '-128'
	option proto 'tcp'
	option target 'Priority'
	option tcpflags 'ACK'

config classgroup 'Default'
	option classes 'Priority Express Normal Bulk'
	option default 'Normal'

config class 'Priority'
	option avgrate '10'
	option maxsize '400'
	option packetsize '400'
	option priority '20'

config class 'Priority_down'
	option avgrate '10'
	option packetsize '1000'

config class 'Express'
	option avgrate '50'
	option maxsize '800'
	option packetsize '1000'
	option priority '10'

config class 'Normal'
	option avgrate '10'
	option packetdelay '100'
	option packetsize '1500'
	option priority '5'

config class 'Normal_down'
	option avgrate '20'

config class 'Bulk'
	option avgrate '1'
	option packetdelay '200'

