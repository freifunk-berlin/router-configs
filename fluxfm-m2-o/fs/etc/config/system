
config system
	option cronloglevel '10'
	option hostname 'fluxfm-m2-o'
	option latitude '52.503123539133382'
	option latlon '52.503123539133382 13.440655315380594'
	option location 'FluxFM'
	option longitude '13.440655315380594'
	option timezone 'CET-1CEST,M3.5.0,M10.5.0/3'
	option zonename 'Europe/Berlin'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enable_server '0'

config led 'led_rssilow'
	option factor '13'
	option iface 'rssid_wlan0'
	option maxq '100'
	option minq '1'
	option name 'RSSILOW'
	option offset '0'
	option sysfs 'ubnt:red:link1'
	option trigger 'rssi'

config led 'led_rssimediumlow'
	option factor '13'
	option iface 'rssid_wlan0'
	option maxq '100'
	option minq '26'
	option name 'RSSIMEDIUMLOW'
	option offset '-25'
	option sysfs 'ubnt:orange:link2'
	option trigger 'rssi'

config led 'led_rssimediumhigh'
	option factor '13'
	option iface 'rssid_wlan0'
	option maxq '100'
	option minq '51'
	option name 'RSSIMEDIUMHIGH'
	option offset '-50'
	option sysfs 'ubnt:green:link3'
	option trigger 'rssi'

config led 'led_rssihigh'
	option factor '13'
	option iface 'rssid_wlan0'
	option maxq '100'
	option minq '76'
	option name 'RSSIHIGH'
	option offset '-75'
	option sysfs 'ubnt:green:link4'
	option trigger 'rssi'

