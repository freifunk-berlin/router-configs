
config interface 'loopback'
	option ifname 'lo'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option proto 'static'

config interface 'lan'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option ifname 'eth0.100'
	option ip6assign '64'
	option ip6prefix '2001:0bf7:0830:ea50:0000:0000:0000:0000/60'
	option ipaddr '104.0.2.5'
	option netmask '255.255.255.240'
	option proto 'static'
	option type 'bridge'

config interface 'wan'
	option ifname 'eth1'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option peerdns '0'
	option proto 'static'

config globals 'globals'
	option ula_prefix 'fdca:b384:87e3::/48'

config route
	option gateway '104.0.2.9'
	option interface 'lan'
	option netmask '255.255.255.0'
	option target '192.168.0.0'

config interface 'wireless0'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option ip6assign '64'
	option ipaddr '104.0.2.205'
	option mtu '1532'
	option netmask '255.0.0.0'
	option proto 'static'

config interface 'wireless0ap'
	option proto 'none'

config interface 'mesh0'
	option ifname 'bat0'
	option proto 'none'

config interface 'bat'
	option ifname 'wlan0 bat0'
	option proto 'none'
	option type 'bridge'

