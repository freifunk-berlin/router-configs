
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11na'
	option macaddr 'dc:9f:db:94:0e:c5'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option country 'DE'
	option diversity '1'
	option outdoor '1'
	option channel '136'
	option htmode 'HT40-'
	option noscan '1'
	option disabled '0'
	option distance '1200'
	option txpower '18'

config wifi-iface
	option device 'radio0'
	option encryption 'none'
	option mode 'ap'
	option network 'wireless0dhcp'
	option ssid 'berlin-5ghz.freifunk.net'

config wifi-iface
	option bgscan '0'
	option bssid '12:36:CA:FF:EE:EE'
	option device 'radio0'
	option encryption 'none'
	option mode 'adhoc'
	option network 'wireless0'
	option probereq '1'
	option ssid 'ch136.freifunk.net'
	option sw_merge '1'

