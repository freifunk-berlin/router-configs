config wifi-device  radio0
	option type     mac80211
	option channel  40
	option hwmode	11na
	option path	'ffe09000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option htmode	HT40-
	list ht_capab	LDPC
	list ht_capab	SHORT-GI-20
	list ht_capab	SHORT-GI-40
	list ht_capab	TX-STBC
	list ht_capab	RX-STBC1
	list ht_capab	DSSS_CCK-40

config wifi-device  radio1
	option type     mac80211
	option channel  10
	option hwmode	11ng
	option path	'ffe0a000.pcie/pci0001:02/0001:02:00.0/0001:03:00.0'
	option country  DE
	option htmode	HT20
	list ht_capab	LDPC
	list ht_capab	SHORT-GI-20
	list ht_capab	SHORT-GI-40
	list ht_capab	TX-STBC
	list ht_capab	RX-STBC1
	list ht_capab	DSSS_CCK-40
	option country 'DE'

config wifi-iface
	option device 	  'radio0'
	option network    'lan'
	option mode       'ap'
	option ssid       'freifunk-astafu-mgmt'
	option encryption 'psk2'

config wifi-iface
	option network 	  'wifi-ap-5ghz'
	option encryption 'none'
	option device     'radio0'
	option mode       'ap'
	option ssid       'freifunk-astafu-5ghz'

config wifi-iface
	option network    'wifi-ap-2ghz'
	option encryption 'none'
	option device     'radio1'
	option mode       'ap'
	option ssid       'freifunk-astafu'

config wifi-iface
	option network    'wifi-olsr'
	option encryption 'none'
	option device     'radio1'
	option bssid      '02:CA:FF:EE:BA:BE'
	option bgscan     '0'
	option probereq   '1'
	option ssid       'olsr.freifunk.net'
	option mode       'adhoc'
	option sw_merge   '1'

