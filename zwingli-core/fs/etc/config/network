
config interface 'loopback'
	option ifname 'lo'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option proto 'static'

config interface 'mesh'
	option ifname 'bat0'
	option ip6assign '64'
	option ipaddr '6.23.16.1'
	option netmask '255.255.240.0'
	option proto 'static'

config interface 'lan'
	option ifname 'eth0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.10.1'
	option netmask '255.255.255.0'
	option macaddr '00:04:9F:EF:10:10'
	option ip6assign '64'
#	list ip6prefix '2001:BF7:F100:1A90:0:0:0:0/61'

config interface 'olsr'
	option proto 'static'
	option ifname 'eth0.1'
	option ipaddr '104.130.10.26'
	option netmask '255.0.0.0'
	option ip6assign '64'
	option dns '2002:d596:2a92:1:71:53:: 2002:5968:c28e::53 88.198.178.18 141.54.1.1 212.204.49.83 8.8.8.8 8.8.4.4'
	option macaddr '00:04:9F:EF:17:17'
	list ip6prefix '2001:bf7:750:1400::/60'

config interface 'olsrbat'
	option ifname '@olsr'
	option mesh 'bat0'
	option proto 'batadv'

config interface 'fhain'
	option ifname 'eth0.3'
